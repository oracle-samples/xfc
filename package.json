{
  "name": "xfc",
  "version": "1.0.0",
  "description": "A Cross Frame Container that handles securely embedding web content into a 3rd party domain",
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run test && npm run clean && npm run lint-js && npm run build-es5 && npm run build-webpack",
    "build-doc": "jsdoc  --package package.json -d doc ./src/**/*.js",
    "build-es5": "babel src --out-dir lib ",
    "build-webpack": "webpack --progress --colors --display-error-details",
    "clean": "rm -rf dist lib doc",
    "dev": "webpack-dev-server --host=0.0.0.0 --inline --config webpack.config.dev.js",
    "lint-js": "eslint 'src/**/*.js' --fix",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "dependencies": {
    "jsonrpc-dispatch": "^2.0.0",
    "mutation-observer": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "jsdoc": "^3.4.0",
    "jsdom": "9.6.0",
    "jsdom-global": "2.1.0",
    "matchdep": "^1.0.1",
    "mocha": "^3.1.0",
    "sinon": "^1.17.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cerner/xfc.git"
  }
}
